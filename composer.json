{
  "name": "juanparati/laravel-odbc",
  "description": "PDO ODBC integration for Laravel with Snowflake and BigQuery support",
  "type": "library",
  "homepage": "https://github.com/juanparati/laravel-odbc",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Abram",
      "email": "andreossido@gmail.com",
      "homepage": "https://github.com/andreossido",
      "role": "Developer"
    },
    {
      "name": "Yoram de Langen",
      "email": "yoramdelangen@gmail.com",
      "homepage": "https://github.com/yoramdelangen",
      "role": "Developer"
    },
    {
      "name": "Juan Lago",
      "email": "juanparati@gmail.com",
      "homepage": "https://github.com/juanparati",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-odbc": "*",
    "ext-pdo_odbc": "*",
    "illuminate/database": ">= 7.0",
    "illuminate/support": ">= 7.0",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "Juanparati\\LaravelOdbc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OdbcServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.18"
  }
}
